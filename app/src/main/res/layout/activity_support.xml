<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:orientation="vertical"
    tools:context=".Support">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@color/orange_startup"
        android:backgroundTint="@color/orange_startup"
        android:drawableLeft="@drawable/ic_baseline_arrow_back_24"
        android:drawableRight="@drawable/ic_baseline_format_list_bulleted_24"
        android:drawablePadding="@dimen/dp_15"
        android:paddingTop="@dimen/dp_30"
        android:paddingBottom="@dimen/dp_15"
        android:text="Support"
        android:textColor="@color/white_startup"
        android:textSize="@dimen/sp_30"
        android:textStyle="bold" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="85dp"
        android:text="General"
        android:textStyle="bold"
        android:textColor="@color/orange_startup"
        android:textSize="@dimen/sp_22"
        android:paddingTop="@dimen/dp_20"
        android:paddingStart="@dimen/dp_15"
        android:drawableEnd="@drawable/ic_loupe"
        android:layout_marginEnd="@dimen/dp_10"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="146dp"
        android:text="Frequently asked questions"
        android:paddingStart="@dimen/dp_20"
        android:textSize="@dimen/sp_18"
        android:textColor="@color/navyblue_body" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-3dp"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="0dp"
        android:paddingStart="@dimen/sp_20"
        android:text="How do i manage and cancel my VIP subscription?"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_18" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="255dp"
        android:layout_marginEnd="-3dp"
        android:paddingStart="@dimen/dp_20"
        android:text="Are there any guidelines for pictures on my profile?"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_18" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="314dp"
        android:layout_marginEnd="-4dp"
        android:paddingStart="@dimen/dp_20"
        android:text="Is Jaumo really for free?"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_18" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="349dp"
        android:layout_marginEnd="-1dp"
        android:paddingStart="@dimen/dp_20"
        android:text="Jaumo Advertising Partners"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_18" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="390dp"
        android:layout_marginEnd="-1dp"
        android:paddingStart="@dimen/dp_20"
        android:text="Why do i not get push notifications?"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_18" />

    <TextView
        android:id="@+id/tVseeAll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="195dp"
        android:paddingStart="@dimen/dp_20"
        android:text="See all 9 articles"
        android:textAllCaps="true"
        android:textColor="@color/orange_startup"
        android:textSize="@dimen/sp_18" />

    <ImageButton
        android:layout_width="@dimen/dp_70"
        android:layout_height="@dimen/dp_70"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="@dimen/dp_50"
        android:layout_marginBottom="@dimen/dp_50"
        android:background="@drawable/circle" />

    <ImageButton
        android:layout_width="@dimen/dp_50"
        android:layout_height="@dimen/dp_50"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/ic_baseline_create_24" />


</RelativeLayout>