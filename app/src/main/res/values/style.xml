<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Gender_Button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/dp_15</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:background">@drawable/rounded_rec_orange</item>
    </style>

    <style name="Filter_TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>

    </style>
<style name="Filter_lineview">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">@dimen/dp_1</item>
    <item name="android:layout_marginLeft">@dimen/dp_20</item>
    <item name="android:layout_marginTop">@dimen/dp_20</item>
    <item name="android:layout_marginRight">@dimen/dp_20</item>
    <item name="android:background">@color/light_gray</item>

</style>

<style name="Filter_LinearLayout">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:orientation">vertical</item>

</style>
</resources>